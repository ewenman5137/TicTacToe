<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="800.0" prefWidth="700.0" style="-fx-background-color: #27265A;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.tictactoe.CustomSingle">
   <children>
      <Label layoutX="272.0" layoutY="35.0" text="TicTacToe" textFill="#f3ebeb">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Label layoutX="256.0" layoutY="95.0" text="Select your Avatar" textFill="#f3ebeb">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Label layoutX="251.0" layoutY="392.0" text="Select your symbol" textFill="#f3ebeb">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Pane fx:id="backgroundPPPlayer1" layoutX="275.0" layoutY="187.0" prefHeight="150.0" prefWidth="150.0" style="-fx-background-color: #1F1818; -fx-background-radius: 100%; -fx-border-color: white; -fx-border-radius: 100%;">
         <children>
            <ImageView fx:id="ppPlayer1" fitHeight="105.0" fitWidth="105.0" layoutX="23.0" layoutY="23.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@dinosaure.png" />
               </image>
            </ImageView>
         </children>
      </Pane>
      <Button fx:id="buttonGoToGrid" layoutX="151.0" layoutY="665.0" mnemonicParsing="false" onAction="#goToTheMatch" prefHeight="75.0" prefWidth="400.0" style="-fx-background-radius: 15px;" text="Back">
         <font>
            <Font size="36.0" />
         </font>
      </Button>
      <Pane fx:id="backgroundPPPlayer11" layoutX="275.0" layoutY="448.0" prefHeight="150.0" prefWidth="150.0" style="-fx-background-color: #1F1818; -fx-background-radius: 100%; -fx-border-color: white; -fx-border-radius: 100%;">
         <children>
            <ImageView fx:id="symbolPlayer1" fitHeight="105.0" fitWidth="105.0" layoutX="23.0" layoutY="22.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../fermer-la-croix.png" />
               </image>
            </ImageView>
         </children>
      </Pane>
      <Button fx:id="buttonLeftPPPlayer1" layoutX="293.0" layoutY="354.0" mnemonicParsing="false" onAction="#leftPPPlayer1">
         <graphic>
            <ImageView fitHeight="47.0" fitWidth="21.0" pickOnBounds="true" preserveRatio="true" rotate="90.0">
               <image>
                  <Image url="@telecharger-arrow.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="buttonRightPPPlayer1" layoutX="370.0" layoutY="354.0" mnemonicParsing="false" onAction="#rightPPPlayer1" rotate="180.0">
         <graphic>
            <ImageView fitHeight="47.0" fitWidth="21.0" pickOnBounds="true" preserveRatio="true" rotate="90.0">
               <image>
                  <Image url="@telecharger-arrow.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="buttonLeftSymbolPlayer1" layoutX="293.0" layoutY="613.0" mnemonicParsing="false" onAction="#leftSymbolPlayer1">
         <graphic>
            <ImageView fitHeight="47.0" fitWidth="21.0" pickOnBounds="true" preserveRatio="true" rotate="90.0">
               <image>
                  <Image url="@telecharger-arrow.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="buttonRightSymbolPlayer1" layoutX="370.0" layoutY="613.0" mnemonicParsing="false" onAction="#rightSymbolPlayer1" rotate="180.0">
         <graphic>
            <ImageView fitHeight="47.0" fitWidth="21.0" pickOnBounds="true" preserveRatio="true" rotate="90.0">
               <image>
                  <Image url="@telecharger-arrow.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Label fx:id="namePlayer" alignment="CENTER" contentDisplay="CENTER" layoutX="254.0" layoutY="140.0" prefHeight="35.0" prefWidth="192.0" text="Name" textFill="#f3ebeb">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
